#!/usr/bin/env python

import sys
import json


for path in sorted(sys.argv[1:]):
    with open(path, 'rb') as fp:
        metadata = json.load(fp)

    name = metadata['name'].replace('/', '-').split('-', 1)[1]
    version = metadata['version']
    print('bundled(puppet-module(%s)) = %s' % (name, version))
